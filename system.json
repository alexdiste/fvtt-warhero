    "hotReload": {
      "extensions": [
        "css",
        "html",
        "hbs",
        "json"
      ],
      "paths": [
        "./",
        "templates",
        "styles",
        "lang/en.json",
        "lang/it.json"
      ]
    }
  },
  "grid": {
    "distance": 1,
    "units": "m"
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "it",
      "name": "Italian",
      "path": "lang/it.json"
    }
  ],
  "primaryTokenAttribute": "attributes.hp",
  "secondaryTokenAttribute": "attributes.mana",
  "socket": true,
    "documentTypes": {
    "Actor": {
      "character": { "htmlFields": ["description", "notes", "gmnotes"] },
      "party": { "htmlFields": ["description", "notes", "gmnotes"] }
    },
    "Item": {
      "armor": { "htmlFields": ["description"] },
      "class": { "htmlFields": ["description"] },
      "classitem": { "htmlFields": ["description"] },
      "equipment": { "htmlFields": ["description"] },
      "weapon": { "htmlFields": ["description"] },
      "competency": { "htmlFields": ["description"] },
      "power": { "htmlFields": ["description"] },
      "language": { "htmlFields": ["description"] },
      "location": { "htmlFields": ["description"] },
      "money": { "htmlFields": ["description"] },
      "condition": { "htmlFields": ["description"] },
      "poison": { "htmlFields": ["description"] },
      "potion": { "htmlFields": ["description"] },
      "race": { "htmlFields": ["description"] },
      "shield": { "htmlFields": ["description"] },
      "skill": { "htmlFields": ["description"] },
      "trap": { "htmlFields": ["description"] }
    }
  },
